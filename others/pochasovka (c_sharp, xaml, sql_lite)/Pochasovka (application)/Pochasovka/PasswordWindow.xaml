<Window x:Class="Pochasovka.PasswordWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Pochasovka"
        mc:Ignorable="d"
        Background="#DCDCDC"
        Title="PasswordWindow" Height="135" Width="310">
    <Grid>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="23" Margin="10,41,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="272" KeyDown="textBox_KeyDown" Focusable="True"/>
        <Button x:Name="button" Content="Ok" HorizontalAlignment="Left" Margin="10,69,0,0" VerticalAlignment="Top" Width="134" Click="button_Click"/>
        <Button x:Name="button1" Content="Cancel" HorizontalAlignment="Left" Margin="149,69,0,0" VerticalAlignment="Top" Width="133" Click="button1_Click"/>
        <Label x:Name="label" Content="Input password" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.795,0.729"/>

    </Grid>
</Window>
